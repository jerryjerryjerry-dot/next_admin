{
  "name": "New-App",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env SKIP_ENV_VALIDATION=true next build",
    "build:prod": "cross-env NODE_ENV=production SKIP_ENV_VALIDATION=true next build",
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "preview": "pnpm build && pnpm start",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:watch": "tsc --noEmit --skipLibCheck --watch",
    "postinstall": "pnpm prisma generate",
    "db:generate": "pnpm prisma generate",
    "db:migrate": "pnpm prisma migrate dev",
    "db:migrate:deploy": "pnpm prisma migrate deploy",
    "db:migrate:reset": "pnpm prisma migrate reset",
    "db:push": "pnpm prisma db push",
    "db:pull": "pnpm prisma db pull",
    "db:studio": "pnpm prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:simple": "tsx prisma/simple-seed.ts",
    "db:seed:enhanced": "tsx prisma/enhanced-seed.ts",
    "db:seed:bulk": "tsx prisma/bulk-seed.ts",
    "db:seed:api-keys": "tsx prisma/test-keys-seed.ts",
    "db:seed:more-keys": "tsx prisma/more-test-keys.ts",
    "db:seed:api-calls": "tsx prisma/api-calls-seed.ts",
    "db:seed:optimized-calls": "tsx prisma/optimized-api-calls-seed.ts",
    "db:seed:ultra-performance": "tsx prisma/ultra-high-performance-seed.ts",
    "db:seed:system-status": "tsx prisma/system-status-seed.ts",
    "db:reset": "pnpx prisma migrate reset --force && pnpm db:seed",
    "test:api": "node test-openapi.mjs",
    "test:trpc": "node test-trpc-openapi.mjs",
    "test:keys": "node test-api-with-keys.mjs",
    "show:keys": "node show-test-keys.mjs"
  },
